<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class name="com.aviara.bean.MonthlyLeaves" table="monthly_leaves">
      <id name="id" column="id" type="integer"> 
          <generator class="increment"></generator> 
      </id> 
        <property name="emp_id" column="emp_id" type="string"></property> 
        <property name="month" column="month" type="integer"></property>
        <property name="year" column="year" type="integer"></property>    
        <property name="isTaken" column="isTaken" type="integer"></property> 
        <property name="total" column="total" type="integer"></property> 
        <property name="leave_id" column="leave_id" type="integer"></property>   
        
    </class>
</hibernate-mapping>