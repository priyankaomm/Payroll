<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >
  
<hibernate-mapping>
<class name="com.aviara.bean.Dbinfo" table="dbinfo">
      <id name="id" column="id" type="integer"> 
          <generator class="increment"></generator> 
      </id> 
        <property name="path" column="path" type="string"></property> 
        <property name="logname" column="logname" type="string"></property> 
        <property name="status" column="status" type="integer"></property> 
        
    </class>
  
</hibernate-mapping>