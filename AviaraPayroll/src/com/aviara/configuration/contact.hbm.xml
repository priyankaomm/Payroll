<?xml version="1.0" encoding="UTF-8"?>

<!--<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">-->
  <!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="com.aviara.bean.Contact" table="contact_info">
      <id name="id" column="id" type="integer"> 
          <generator class="increment"></generator> 
      </id> 
        <property name="contact_no" column="contact_no" type="string"></property> 
        <property name="alt_contact" column="alt_contact" type="string"></property>   
        <property name="email_id" column="email_id" type="string"></property> 
        <property name="guardian_name" column="guardian_name" type="string"></property> 
        <property name="g_contact" column="guardian_contact" type="string"></property>   
        <property name="mother_name" column="mother_name" type="string"></property> 
        <property name="m_contact" column="mother_contact" type="string"></property> 
        <property name="emergency1_name" column="emergency1_name" type="string"></property>   
        <property name="emergency2_name" column="emergency2_name" type="string"></property>
        <property name="emergency1" column="emergency1" type="string"></property>   
        <property name="emergency2" column="emergency2" type="string"></property>
        <property name="emp_id" column="emp_id" type="string"></property>
        <!--<many-to-one name="emp_id" class="com.aviara.bean.Employee"
			column="emp_id" unique="true" not-null="true" 
			cascade="all" />-->
    </class>
  
  

</hibernate-mapping>
