<?xml version="1.0" encoding="UTF-8"?>

<!--<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">-->

  <!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
  <class name="com.aviara.bean.Qualification" table="qtemp">
      <id name="id" column="id" type="integer"> 
          <generator class="increment"></generator> 
      </id> 
        <property name="degree" column="degree" type="string"></property> 
        <property name="university" column="university" type="string"></property>   
        <property name="collage_name" column="collage_name" type="string"></property> 
        <property name="marks" column="marks" type="double"></property> 
        <property name="emp_id" column="emp_id" type="string"></property>
    </class>
  

</hibernate-mapping>
