<?xml version="1.0" encoding="UTF-8"?>

<!--<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">-->
<!DOCTYPE hibernate-mapping SYSTEM "hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="com.aviara.bean.Address" table="address_info">
      <id name="id" column="id" type="integer"> 
          <generator class="increment"></generator> 
      </id> 
        <property name="country1" column="country1" type="string"></property> 
        <property name="state1" column="state1" type="string"></property>   
        <property name="city1" column="city1" type="string"></property> 
        <property name="pincode1" column="pincode1" type="string"></property> 
        <property name="address1" column="address1" type="string"></property>   
        <property name="country2" column="country2" type="string"></property> 
        <property name="state2" column="state2" type="string"></property> 
        <property name="city2" column="city2" type="string"></property>   
        <property name="address2" column="address2" type="string"></property>
        <property name="emp_id" column="emp_id" type="string"></property>
    </class>
  
  

</hibernate-mapping>
